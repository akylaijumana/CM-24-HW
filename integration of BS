#include <iostream>
#include <cmath>
using namespace std;

double integration(double x) {
    return x; 
}
double drand(int d){
    return((rand()%1000)/1000.0)*d;

int main() {
    double a = 0.0; 
    double b = 1.0;  
    int n = 100;    
    double I = 0.0; 
    int K = 0;       

    srand(time(0));  

    while (K < n) {
        double r = (b - a) * drand(b-a) + a;  
        I += integration(r) * (b - a);  
        K++;
    }
    cout << I / K << endl; 
    return 0;
}
